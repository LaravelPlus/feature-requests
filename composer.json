{
    "name": "laravelplus/feature-requests",
    "description": "A comprehensive Laravel feature requests package with voting system, categories, and status management",
    "keywords": [
        "laravel",
        "feature-requests",
        "voting",
        "feedback",
        "user-requests",
        "product-management"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "LaravelPlus Team",
            "email": "team@laravelplus.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-sluggable": "^3.0",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/events": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelPlus\\FeatureRequests\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelPlus\\FeatureRequests\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelPlus\\FeatureRequests\\Providers\\FeatureRequestsServiceProvider"
            ],
            "aliases": {
                "FeatureRequests": "LaravelPlus\\FeatureRequests\\FeatureRequestsFacade"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "pint": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
